      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "link": {
      "type": "string",
      "unique": true,
      "minLength": 10
    },
    "introduction": {
      "type": "text",
      "unique": true,
      "minLength": 10,
      "maxLength": 1000
    },
    "message_link": {
      "type": "string",
      "unique": true,
      "minLength": 9
    },
    "video": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "videos"
      ],
      "plugin": "upload",
